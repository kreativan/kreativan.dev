---
import { getCollection, getEntry } from "astro:content";
---

<div class="uk-grid uk-grid-large" uk-grid>
  {
    services.map((item) => (
      <div class="uk-width-1-3@m">
        <h3 class="uk-margin-small">{item.title}</h3>
        <p class="uk-margin-small" set:html={item.text} />
      </div>
    ))
  }
</div>
